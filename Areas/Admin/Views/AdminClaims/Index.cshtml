@model IEnumerable<IdentityUser>

@{
    ViewData["Title"] = "Index";
}

<h2>Usuários</h2>
@if (Model.Any())
{
    <table class="table table-sm table-bordered">
        <tr>
            <th>ID</th>
            <th>Nome</th>
        </tr>
        @foreach (var use in Model)
        {
            <tr>
                <td>@use.Id</td>
                <td>@use.Email</td>
                <td>
                    <form asp-action="EditUser" asp-route-id="@use.Id"method="get">
                        <button type="submit" class="btn btn-sm btn-primary">
                            Gerenciar Claims
                        </button>
                    </form>
                </td>
            </tr>
            
        }
    </table>
    
}